                                <view class="lineMe"></view>
                                <view class="area margin">
                                    <view class="leftMe" style="width:100%;">
                                        <image src="/images/icon/phone.png"></image>
                                        <text>电话：{{resDetailData.resPhone}}</text>
                                    </view>
                                </view>
                                <view class="area padding">
                                    <view class="leftMe" style="width:100%;">
                                        <image src="/images/icon/dingwei.png"></image>
                                        <text>地址：{{resDetailData.resAddress}}</text>
                                    </view>
                                </view>
                            </view>
                        </view>
                    </view>
                </swiper-item>
            </swiper>
        </view>

        <!--未结算弹框-->
        <view class="module {{module}}">
            <view class="tanchuang">
                <text class="p content">您有未结算的订单</text>
                <view class="h4">
                    <button class="fl use" bindtap="pay">进入订单</button>
                    <button class="fr phone" bindtap="addDish">取消</button>
                    <view class="clear"></view>
                </view>
            </view>
            <view class="clear"></view>
        </view>

        <!--loading弹框-->
        <import src="../../../template/loading.wxml"/>
        <template is="loading" data="{{isLoading}}"></template>

        <!--规格弹窗-->
        <import src="../../../template/module/index.wxml"/>
        <view bindtap="modulePopup">
            <template is="module-popup" data="{{...ShopOneData,class:moduleActiveMe,animated,isMask}}"></template>
        </view>
    </view>
</view>