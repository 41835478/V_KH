<template name="food-table">
    <view class="image flexBox-item fb-4 food-left">
        <image src="{{items.foodImage?(serverAddressImg+items.foodImage):'/images/noImg.png'}}"></image>
    </view>
    <view class="flexBox-item fb-8 food-right overflow-h pr">
        <view class="p text-overflow">
            <text>{{items.name}}</text>
        </view>
        <view class="p text-overflow">月售 {{items.salesCount}}</view>
        <view class="p clearfix">
            <view class="fl pr food-money bold">
                <text>¥{{items.price}}</text>
            </view>
            <view class="fr pr food-btn-plus clearfix">
                <block wx:if="{{items.foodRuleCount>1||items.foodPracticesCount>1}}">
                    <view class="height-all food-rule"
                          data-ccc="{{items.foodRuleCount}}">
                        <button class="btn btn-red-default height-all food-btn-rule"
                                data-active="setRule"
                                data-value="{{items}}"
                                data-picktype="0"
                                data-index="{{index+','+i}}">选规格
                        </button>
                    </view>
                </block>
                <block wx:else>
                    <view hidden="{{!(hallCounts[items.foodCode]>0)}}"
                          bindtap="removeToCarts"
                          data-food="{{items}}"
                          data-pickType="0"
                          data-ruleCode="{{items.ruleCode}}"
                          class="reduce choose-area-box-left">
                        <image src="/images/icon/reduce.png"></image>
                    </view>
                    <view class="sum text-center-box"
                          hidden="{{!(hallCounts[items.foodCode]>0)}}">
                        <text>{{hallCounts[items.foodCode]}}
                        </text>
                    </view>
                    <view wx:if="{{plus}}"
                          class="choose-area-box-right"
                          data-food="{{items}}"
                          data-pickType="0" bindtap="addToCarts"
                          class="plus choose-area-box-right"
                          data-ruleCode="{{items.ruleCode}}">
                        <image src="/images/icon/plus.png"></image>
                    </view>
                    <view class="plus" wx:else bindtap="noPlus">
                        <image src="/images/icon/noPlus.png"></image>
                    </view>
                </block>
            </view>
        </view>
    </view>
</template>


