.picker-view {
  position: fixed;
  z-index: 500;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #fff;
}
.picker-view .picker-view-title {
  padding: 20rpx 30rpx;
  position: relative;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  border-bottom: 2rpx solid #e2e2e2;
  text-align: center;
}
.picker-view .picker-view-footer {
  border-radius: 0;
  padding: 20rpx 30rpx;
  background-color: #fff;
  line-height: 2;
  position: relative;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  border-top: 2rpx solid #e2e2e2;
  text-align: center;
}
.picker-view .picker-view-content {
  max-height: 800rpx;
  min-height: 200rpx;
  background-color: #f8f8f8;
}
.picker-view .picker-view-content scroll-view {
  max-height: 800rpx;
}
.picker-view .picker-view-content scroll-view .pickerViewPayment {
  min-height: 200rpx;
}
.picker-view .picker-view-content scroll-view .pickerViewPayment view {
  box-sizing: border-box;
}
.picker-view .picker-view-content scroll-view .pickerViewPayment .pickerViewPayment-item {
  position: relative;
  width: 100%;
  box-sizing: border-box;
  background-color: #fff;
  padding: 20rpx 30rpx;
  font-size: 28rpx;
}
.picker-view .picker-view-content scroll-view .pickerViewPayment .pickerViewPayment-item:nth-child(1) {
  border-bottom: 1rpx solid #e2e2e2;
}
.picker-view .picker-view-content scroll-view .pickerViewPayment .pickerViewPayment-item .flexBox-item {
  height: 88rpx;
}
.picker-view .picker-view-content scroll-view .pickerViewPayment .pickerViewPayment-item .weChat-left {
  line-height: 88rpx;
}
.picker-view .picker-view-content scroll-view .pickerViewPayment .pickerViewPayment-item .btn-right {
  height: 88rpx;
  padding: 16rpx 0;
}
.picker-view .picker-view-content scroll-view .pickerViewPayment .pickerViewPayment-weChat .azm-icon {
  font-size: 34rpx;
  color: #0bb20c;
}
.picker-view .picker-view-content scroll-view .pickerViewPayment .pickerViewPayment-weChat .azm-btn {
  line-height: 2;
  padding-left: 20rpx;
  padding-right: 20rpx;
  margin: 0;
}
.picker-view .picker-view-content scroll-view .pickerViewPayment .pickerViewPayment-member {
  position: relative;
  top: 0;
}
.picker-view .picker-view-content scroll-view .pickerViewPayment .pickerViewPayment-member .azm-icon {
  font-size: 30rpx;
  color: #f74b7b;
}
.picker-view .picker-view-content scroll-view .pickerViewPayment .pickerViewPayment-member .azm-btn {
  line-height: 2;
  padding-left: 20rpx;
  padding-right: 20rpx;
  margin: 0;
}
.picker-view .picker-view-content scroll-view .pickerViewPayment .pickerViewPayment-member .payment-member-content {
  position: relative;
  width: 100%;
  padding-left: 60rpx;
  color: #808080;
  font-size: 24rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.picker-view .picker-view-content scroll-view .picker-view-payment {
  min-height: 200rpx;
  padding: 0 30rpx;
}
.picker-view .picker-view-content scroll-view .picker-view-payment .picker-view-payment-item {
  position: relative;
  width: 100%;
  box-sizing: border-box;
  background-color: #fff;
  padding: 20rpx 30rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  border-radius: 6rpx;
  border: 1rpx solid #e2e2e2;
}
.picker-view .picker-view-content scroll-view .picker-view-payment .picker-view-payment-weChat .azm-icon {
  color: #0bb20c;
}
.picker-view .picker-view-content scroll-view .picker-view-payment .picker-view-payment-weChat .flexBox-item {
  height: 60rpx;
  line-height: 60rpx;
}
.picker-view .picker-view-content scroll-view .picker-view-payment .picker-view-payment-weChat .azm-btn {
  line-height: 2;
  padding-left: 20rpx;
  padding-right: 20rpx;
  margin: 0;
}
.picker-view .picker-view-content scroll-view .picker-view-payment .picker-view-payment-member {
  position: relative;
  top: 0;
}
.picker-view .picker-view-content scroll-view .picker-view-payment .picker-view-payment-member::after {
  content: '';
  position: absolute;
  margin-top: -10rpx;
  top: 50%;
  left: -10rpx;
  width: 20rpx;
  height: 20rpx;
  background-color: #f8f8f8;
  border-radius: 100%;
  border-top: 1rpx solid #e2e2e2;
  border-right: 1rpx solid #e2e2e2;
  transform: rotateZ(45deg);
}
.picker-view .picker-view-content scroll-view .picker-view-payment .picker-view-payment-member::before {
  content: '';
  position: absolute;
  margin-top: -10rpx;
  top: 50%;
  right: -10rpx;
  width: 20rpx;
  height: 20rpx;
  background-color: #f8f8f8;
  border-radius: 100%;
  border-top: 1rpx solid #e2e2e2;
  border-left: 1rpx solid #e2e2e2;
  transform: rotateZ(-45deg);
}
.picker-view .picker-view-content scroll-view .picker-view-payment .picker-view-payment-member .flexBox-item {
  height: 60rpx;
  line-height: 60rpx;
}
.picker-view .picker-view-content scroll-view .picker-view-payment .picker-view-payment-member .azm-icon {
  color: #f74b7b;
}
.picker-view .picker-view-content scroll-view .picker-view-payment .picker-view-payment-member .azm-btn {
  line-height: 2;
  padding-left: 20rpx;
  padding-right: 20rpx;
  margin: 0;
}
.picker-view .picker-view-content scroll-view .picker-view-payment .picker-view-payment-member .payment-member-content {
  position: relative;
  width: 100%;
  margin-top: 20rpx;
  padding-left: 20rpx;
  line-height: 60rpx;
  color: #808080;
}
.picker-view .picker-view-content scroll-view .picker-view-payment .picker-view-payment-member .payment-member-content .flexBox-item {
  font-size: 24rpx;
}
.picker-view .picker-view-content scroll-view .picker-view-orderTracking {
  background-color: #fff;
  box-sizing: border-box;
  padding: 0 40rpx;
}
.picker-view .picker-view-content scroll-view .picker-view-orderTracking .picker-view-orderTracking-item .flexBox-item {
  position: relative;
  height: 104rpx;
  line-height: 104rpx;
