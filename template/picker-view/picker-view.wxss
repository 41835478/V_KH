  font-size: 26rpx;
}
.picker-view .picker-view-content scroll-view .picker-view-orderTracking .picker-view-orderTracking-item .flexBox-item .azm-icon {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 14rpx;
  color: #c5c5c5;
}
.picker-view .picker-view-content scroll-view .picker-view-orderTracking .picker-view-orderTracking-item .flexBox-item .azm-icon-active {
